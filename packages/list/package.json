{
  "name": "@equinor/fusion-wc-list",
  "version": "1.1.4",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -b",
    "postbuild": "pnpm typedoc",
    "typedoc": "cem analyze --litelement --globs \"src/**/*.ts\" --outdir lib",
    "prepack": "pnpm build && fwc-build"
